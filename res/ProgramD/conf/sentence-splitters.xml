<?xml version="1.0" encoding="ISO-8859-1"?>
<!--Sentence splitters define strings that mark the end of a sentence,
    after input substitutions have been performed.-->
<sentence-splitters xmlns="http://aitools.org/programd/4.6/bot-configuration"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://aitools.org/programd/4.6/bot-configuration http://aitools.org/programd/4.6/bot-configuration.xsd">
    <splitter value="."/>
    <splitter value="!"/>
    <splitter value="?"/>
    <splitter value=";"/>
</sentence-splitters>
